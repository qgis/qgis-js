{
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "engines": {
    "node": ">=16.0.0",
    "emsdk": "3.1.29",
    "vcpkg": "2023.10.19",
    "qt": "6.5.2",
    "qgis": "3.32.1"
  },
  "type": "module",
  "scripts": {
    "compile": "pnpm run compile:dev",
    "compile:dev": "./compile.ts",
    "compile:debug": "./compile.ts --buildType Debug",
    "compile:release": "./compile.ts --buildType Release",
    "build": "pnpm -r --filter=./packages/** run build",
    "dev": "pnpm --filter @qgis-js/dev dev",
    "site": "vite dev",
    "deploy": "pnpm -r --filter=./sites/** run deploy",
    "lint": "npm run lint:prettier && npm run lint:clang-format",
    "lint:prettier": "npx prettier . --write",
    "lint:pretty-quick": "pretty-quick --staged",
    "lint:clang-format": "clang-format -i \"--glob=src/**/*.{cpp,hpp}\""
  },
  "devDependencies": {
    "pnpm": "8.7.5",
    "vite": "4.5.0",
    "vite-node": "0.34.6",
    "zx": "7.2.3",
    "prettier": "3.0.3",
    "pretty-quick": "3.1.3",
    "clang-format": "1.8.0"
  }
}
